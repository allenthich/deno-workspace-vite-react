{
  "tasks": {
    "generate": "deno run -A npm:kubb@3.0.0-beta.9 generate"
  },
  "workspace": {
    "members": [
      "apps/be-sample",
      "apps/fe-sample",
      "libs/pets"
    ]
},
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "npm:react@^18.3",
    "jsxImportSourceTypes": "npm:@types/react@^18.3"
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@kubb/core": "npm:@kubb/core@3.0.0-beta.9",
    "@kubb/plugin-client": "npm:@kubb/plugin-client@3.0.0-beta.9",
    "@kubb/plugin-oas": "npm:@kubb/plugin-oas@3.0.0-beta.9",
    "@kubb/plugin-react-query": "npm:@kubb/plugin-react-query@3.0.0-beta.9",
    "@kubb/plugin-ts": "npm:@kubb/plugin-ts@3.0.0-beta.9",
    "@kubb/plugin-zod": "npm:@kubb/plugin-zod@3.0.0-beta.9",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.59.16",
    "axios": "npm:axios@^1.7.7",
    "vite-tsconfig-paths": "npm:vite-tsconfig-paths@^5.0.1",
    "zod": "npm:zod@^3.23.8"
  }
}
